{
    "name": "jiny/laravel",
    "description": "jiny Laravel",
    "keywords": [
        "jinyPHP",
        "framwork"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Laravel\\":"/"
        },
        "files" : [
            "Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Laravel\\JinyLaravelServiceProvider"
            ],
            "publish": {
                "resources/views/welcome.blade.php": "resources/views/welcome.blade.php"
            }
        }
    }
}
