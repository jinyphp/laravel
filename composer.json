{
    "name": "jiny/laravel",
    "description": "jiny Laravel",
    "keywords": [
        "jinyPHP",
        "framwork"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "jiny/locale": "^0.3.2",
        "jiny/manual": "^0.0.2",
        "jiny/modules": "^0.3.2",
        "jiny/admin": "^0.4.0",
        "jiny/auth": "^0.10"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Laravel\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Laravel\\JinyLaravelServiceProvider"
            ]
        }
    }
}
